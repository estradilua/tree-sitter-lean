{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lean",
      "camelcase": "Lean",
      "title": "Lean",
      "scope": "source.lean",
      "file-types": [
        "lean"
      ],
      "injection-regex": "^lean$",
      "class-name": "TreeSitterLean"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "GPL-3.0-or-later",
    "description": "Lean grammar for tree-sitter",
    "authors": [
      {
        "name": "Lua Reis",
        "email": "me@lua.blog.br",
        "url": "https://lua.blog.br/"
      }
    ],
    "links": {
      "repository": "https://github.com/estradilua/tree-sitter-lean"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}